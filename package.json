{
  "name": "@istanbul/ws",
  "version": "0.0.2",
  "description": "Websocket packages of istanbul framework",
  "homepage": "https://github.com/istanbulnode/ws#readme",
  "keywords": [
    "typescript",
    "istanbul",
    "websocket",
    "versioned"
  ],
  "author": "Sami Salih İBRAHİMBAŞ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istanbulnode/ws.git"
  },
  "bugs": {
    "url": "https://github.com/istanbulnode/ws/issues"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --watchAll --no-cache",
    "clear": "del-cli ./dist/*",
    "build": "npm run clear && tsc",
    "demo": "ts-node-dev ./demo/demo.ts"
  },
  "dependencies": {
    "@istanbul/app": "^0.0.4",
    "@istanbul/core": "^0.0.5",
    "@istanbul/events": "^0.0.5",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/$1",
      "~/(.*)": "<rootDir>/$1"
    }
  }
}
